<Window x:Class="PrintLabel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="打印插件"
        FontSize="12"
        MinWidth="750"
        MinHeight="450"
        Background="#FFF0F0F0"
        Icon="Resources/WindowIcon.ico">

    <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden" Loaded="mainFrame_Loaded">
        <Frame.Content>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
                <Image x:Name="iconImage" MaxWidth="256" MaxHeight="256" Stretch="UniformToFill" Source="Resources/WindowIcons/Icon 256x256.png"></Image>
                <TextBlock Text="程序初始化 ..." HorizontalAlignment="Center" FontSize="20" Margin="0,20,0,0"/>
            </StackPanel>
        </Frame.Content>
    </Frame>
</Window>